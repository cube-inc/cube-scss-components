{
  "name": "cube-scss-components",
  "version": "2.0.0-beta.7",
  "description": "Basic SCSS components of Cube web apps",
  "author": "Fred Gomez <fred@cube.rentals>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/cube-inc/cube-scss-components.git"
  },
  "keywords": [
    "scss"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:css": "sass src/scss/index.scss build/index.css --style=compressed",
    "build:css:dev": "sass src/scss/index.scss build/index.css",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "files": [
    "*.md",
    "src/scss/**/*.md",
    "src/scss/**/*.scss"
  ],
  "dependencies": {},
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-unit-jest": "^3.0.3",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^5.0.0",
    "prettier": "^2.0.5",
    "sass": "^1.26.3",
    "sass-loader": "^7.1.0",
    "v-clipboard": "^2.2.1",
    "vue": "^2.6.6",
    "vue-template-compiler": "^2.5.21"
  }
}
